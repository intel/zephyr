#include <dt-bindings/pcie/pcie.h>

/ {
	pcie0 {
		label = "PCIE_0";
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "intel,pcie";
		ranges;

		ivshmem0: ivshmem@800 {
			compatible = "qemu,ivshmem";

			reg = <PCIE_BDF_NONE PCIE_ID(0x1af4,0x1110)>;
			label = "IVSHMEM";
			status = "okay";
		};
	};
};
